#include <stdio.h>
#define max 10
#define data_type int
typedef struct Stack{
    data_type top;
    data_type array[max];
 }stack;
void initStack(stack *s)
{

s->top=-1;

}
int isEmpty(struct Stack* stack)
{
    if(stack->top == -1)
    return 1;
    else
    return 0;
}
int stackOverFlow(stack *stack)
{
     if(stack->top == max-1)
     return 1;
     else
     return 0;
}

void push(int val,stack *stack)
{
    if(!stackOverFlow(stack))
     stack->array[++stack->top] = val;
     else
     printf("stackOverFlow\n");
     return;
}
int pop(stack *stack)
{

   if (isEmpty(stack))
   {
       printf("stack is empty\n");
        return -1;
   }
    return stack->array[stack->top--];
}


int main()
{

	stack s;
	initStack(&s);
	pop(&s);
	push(1,&s);
	push(2,&s);
	push(3,&s);
    push(4,&s);
    push(5,&s);
    push(6,&s);
    push(7,&s);

    push(8,&s);
    push(9,&s);

    push(10,&s);
    push(11,&s);
  


      pop(&s);
     
        while(isEmpty(&s)!=1)
        {
          printf("%d  ",pop(&s)); 
        }
    return 0;
}
